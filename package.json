{
  "name": "aerotage-time-reporting-api",
  "version": "1.0.0",
  "description": "Aerotage Time Reporting Application - AWS Serverless Backend API",
  "main": "dist/index.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "cdk": "cdk",
    "predeploy": "npm run build",
    "deploy:dev": "cdk deploy --all --profile aerotage-dev --require-approval never",
    "deploy:staging": "cdk deploy --all --profile aerotage-staging --require-approval never",
    "deploy:prod": "cdk deploy --all --profile aerotage-prod",
    "destroy:dev": "cdk destroy --all --profile aerotage-dev",
    "destroy:staging": "cdk destroy --all --profile aerotage-staging",
    "destroy:prod": "cdk destroy --all --profile aerotage-prod",
    "diff": "cdk diff",
    "synth": "cdk synth",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "keywords": [
    "aws",
    "cdk",
    "serverless",
    "time-tracking",
    "aerotage"
  ],
  "author": "Aerotage Design Group, Inc",
  "license": "ISC",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.143",
    "@types/jest": "^29.5.12",
    "@types/node": "^22.15.21",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.686.0",
    "@aws-sdk/client-dynamodb": "^3.686.0",
    "@aws-sdk/client-s3": "^3.686.0",
    "@aws-sdk/client-ses": "^3.686.0",
    "aws-cdk-lib": "^2.169.0",
    "aws-lambda": "^1.0.7",
    "constructs": "^10.3.0",
    "uuid": "^11.0.3"
  }
}
